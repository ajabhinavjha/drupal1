<section id="about" class="about section">

	<div class="container">

		<div class="row gy-4">

			<div class="col-lg-6 content " data-aos="fade-up" data-aos-delay="100">
				{% include "@atoms/paragraph/paragraph.twig" with {
    para_content: whowepara,
} %}
				{% include "@atoms/heading3/heading3.twig" with {
    heading3_content: whoweheading,
} %}
				{% include "@atoms/paragraph/paragraph.twig" with {
    para_content: whowepara1,
    modifier_class: whowepara1style,
} %}
				<ul>
	{% for item in whowebiitems %}
		{% include "@atoms/list/list.twig" with {
    li_biclass: item.whowebi,
    list_content: item.whowelist,
} %}
	{% endfor %}
</ul>
			</div>

			<div class="col-lg-6 about-images " data-aos="fade-up" data-aos-delay="200">
				<div class="row gy-4">
					<div class="col-lg-6">
						{% include "@atoms/image/image.twig" with {
    image_src: whoweimg,
} %}
					</div>
					<div class="col-lg-6">
						<div class="row gy-4">
							<div class="col-lg-12 d-flex flex-column gap-3">
								{% for item in whoweitems %}
	{% include "@atoms/image/image.twig" with {
    image_src: item.whoweimg,
} %}
{% endfor %}
							</div>
						</div>
					</div>
				</div>

			</div>

		</div>

	</div>
</section>
