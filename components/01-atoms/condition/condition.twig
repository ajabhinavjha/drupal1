{% if img %}
  <img src="{{ img }}" alt="">
{% elseif video %}
  <video src="{{ video }}" controls></video>
{% else %}
  <iframe src="https://www.youtube.com/embed/{{ iframe }}" frameborder="0" width="560" height="312" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
{% endif %}
